{
  "name": "fadirect",
  "version": "1.0.0",
  "description": "Funeral Arranger Direct - Secure communication platform for funeral arrangers and mourners",
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest --passWithNoTests",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "postinstall": "patch-package",
    "check": "node scripts/check-prerequisites.js",
    "build:ios:preview": "eas build --platform ios --profile preview",
    "build:android:preview": "eas build --platform android --profile preview",
    "build:preview": "eas build --platform all --profile preview",
    "build:ios:prod": "eas build --platform ios --profile production",
    "build:android:prod": "eas build --platform android --profile production",
    "build:prod": "eas build --platform all --profile production",
    "submit:ios": "eas submit --platform ios --latest",
    "submit:android": "eas submit --platform android --latest",
    "submit:all": "eas submit --platform all --latest"
  },
  "dependencies": {
    "@privacyresearch/libsignal-protocol-typescript": "^0.0.11",
    "@react-native-async-storage/async-storage": "~1.23.1",
    "@react-native-community/datetimepicker": "8.0.1",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "@reduxjs/toolkit": "^2.0.1",
    "@supabase/supabase-js": "^2.39.0",
    "date-fns": "^3.0.6",
    "date-fns-tz": "^3.2.0",
    "expo": "~51.0.0",
    "expo-build-properties": "~0.12.0",
    "i18n-js": "^4.3.2",
    "react": "18.2.0",
    "react-native": "0.74.5",
    "react-native-document-picker": "^9.1.1",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-get-random-values": "~1.11.0",
    "react-native-image-picker": "^7.1.0",
    "react-native-localize": "^3.0.6",
    "react-native-paper": "^5.11.6",
    "react-native-progress": "^5.0.1",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "~3.31.1",
    "react-native-svg": "~15.2.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-vector-icons": "^10.0.3",
    "react-redux": "^9.0.4",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/plugin-transform-class-properties": "^7.28.6",
    "@babel/plugin-transform-private-methods": "^7.28.6",
    "@babel/plugin-transform-private-property-in-object": "^7.28.6",
    "@babel/preset-env": "^7.24.0",
    "@babel/runtime": "^7.24.0",
    "@react-native/eslint-config": "^0.74.0",
    "@react-native/metro-config": "^0.74.0",
    "@react-native/typescript-config": "^0.74.0",
    "@testing-library/react-native": "^12.4.3",
    "@types/react": "~18.2.79",
    "@types/uuid": "^9.0.7",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "metro-react-native-babel-preset": "^0.77.0",
    "patch-package": "^8.0.0",
    "prettier": "^3.1.1",
    "react-test-renderer": "18.2.0",
    "typescript": "~5.3.3"
  },
  "engines": {
    "node": ">=18",
    "npm": ">=9"
  },
  "overrides": {
    "eslint-plugin-prettier": "^5.2.1"
