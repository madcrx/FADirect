{
  "name": "fadirect",
  "version": "1.0.0",
  "description": "Funeral Arranger Direct - Secure communication platform for funeral arrangers and mourners",
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "postinstall": "patch-package",
    "check": "node scripts/check-prerequisites.js",
    "build:ios:preview": "eas build --platform ios --profile preview",
    "build:android:preview": "eas build --platform android --profile preview",
    "build:preview": "eas build --platform all --profile preview",
    "build:ios:prod": "eas build --platform ios --profile production",
    "build:android:prod": "eas build --platform android --profile production",
    "build:prod": "eas build --platform all --profile production",
    "submit:ios": "eas submit --platform ios --latest",
    "submit:android": "eas submit --platform android --latest",
    "submit:all": "eas submit --platform all --latest"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.73.6",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-reanimated": "~3.6.1",
    "react-native-safe-area-context": "~4.8.2",
    "react-native-screens": "~3.29.0",
    "@react-native-firebase/app": "^19.0.1",
    "@react-native-firebase/auth": "^19.0.1",
    "@react-native-firebase/firestore": "^19.0.1",
    "@react-native-firebase/storage": "^19.0.1",
    "@react-native-firebase/messaging": "^19.0.1",
    "@react-native-firebase/functions": "^19.0.1",
    "react-native-paper": "^5.11.6",
    "react-native-vector-icons": "^10.0.3",
    "@react-native-community/datetimepicker": "^7.6.2",
    "react-native-document-picker": "^9.1.1",
    "react-native-image-picker": "^7.1.0",
    "react-native-fs": "^2.20.0",
    "@reduxjs/toolkit": "^2.0.1",
    "react-redux": "^9.0.4",
    "@privacyresearch/libsignal-protocol-typescript": "^0.0.11",
    "react-native-get-random-values": "~1.8.0",
    "uuid": "^9.0.1",
    "date-fns": "^3.0.6",
    "date-fns-tz": "^3.2.0",
    "react-native-localize": "^3.0.6",
    "i18n-js": "^4.3.2",
    "react-native-progress": "^5.0.1",
    "react-native-svg": "~14.1.0",
    "expo": "~50.0.0",
    "expo-build-properties": "~0.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.7",
    "@babel/runtime": "^7.23.7",
    "@react-native/eslint-config": "^0.73.2",
    "@react-native/metro-config": "^0.73.3",
    "@react-native/typescript-config": "^0.73.1",
    "@types/react": "~18.2.45",
    "@types/uuid": "^9.0.7",
    "typescript": "^5.3.3",
    "jest": "^29.7.0",
    "@testing-library/react-native": "^12.4.3",
    "react-test-renderer": "18.2.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "metro-react-native-babel-preset": "^0.77.0",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "patch-package": "^8.0.0"
  },
  "engines": {
    "node": ">=18",
    "npm": ">=9"
  }
}
